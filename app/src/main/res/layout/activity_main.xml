<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/buttonCreate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Create"
         />

    <Button
        android:id="@+id/buttonGet"
        android:layout_below="@+id/buttonCreate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Get"
        />

    <Button
        android:id="@+id/buttonDelete"
        android:layout_below="@+id/buttonGet"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Delete"
        />


    <Button
        android:id="@+id/buttonGetMany"
        android:layout_below="@+id/buttonDelete"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Get many"
        />

    <Button
        android:id="@+id/buttonExists"
        android:layout_below="@+id/buttonGetMany"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Exists"
        />

    <Button
        android:id="@+id/buttonUpdate"
        android:layout_below="@+id/buttonExists"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Update"
        />

</RelativeLayout>